<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.model.mapper.testDeleteMapper">
    <delete id="deleteAllOptions">
        delete from test_question_options where TqNo = #{TqNo}
    </delete>
    <delete id="deleteQuestion">
        delete from test_question where TqNo = #{TqNo}
    </delete>

    <delete id="deletePaper">
        delete from test_paper where TpNo=#{TpNo}
    </delete>
</mapper>